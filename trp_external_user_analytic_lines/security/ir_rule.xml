<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="rule_restrict_analytic_line" model="ir.rule">
            <field name="name">Access to own partners' analytic lines</field>
            <field name="model_id" ref="analytic.model_account_analytic_line" />
            <field name="domain_force">[
                    ('account_id.partner_id', 'in', [p.id for p in user.external_user_partner_ids]),
                ]
            </field>
            <field name="groups" eval="[(6, 0, [ref('trp_external_user_analytic_lines.group_external_user_analytic_lines')])]" />
        </record>
    </data>
</openerp>
