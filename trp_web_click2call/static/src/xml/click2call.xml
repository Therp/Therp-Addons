<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
<t t-name="click2call">
    <table cellpadding="0" cellspacing="0" border="0" width="100%">
    <tr>
        <td width="100%">
            <t t-call="FieldChar"/>
        </td>
        <td width="16">
            <button class="oe_button oe_field_button" type="button" title="Call with click2call">
